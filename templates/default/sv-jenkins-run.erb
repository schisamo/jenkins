#!/bin/sh
cd <%= @options[:home] %>
touch jenkins.start
exec chpst -u <%= @options[:user] %> -U <%= @options[:user] %> \
  env HOME=<%= @options[:home] %> \
  JENKINS_HOME=<%= @options[:data_dir] %> \
  java <%= @options[:jvm_options] %> -jar <%= @options[:war_path] %> \
  -Xloggc:<%= @options[:log_dir] %>/gclog.log \
  --httpPort=<%= @options[:port] %>
